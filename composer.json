{
    "name": "henryavila/moeda-real",
    "description": "Laravel Nova Field para campos de moeda em Real (Brasil).",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Henryavila\\MoedaReal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henryavila\\MoedaReal\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
